<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Global Footer Links"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem6_2_docs_component_en#Image - Sightly">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <navigation
                jcr:primaryType="nt:unstructured"
                jcr:title="Footer Navigation"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    sling:resourseType="granite/ui/components/foundation/multifield"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <company
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Select Company link"
                                name="./companyLink"
                                rootPath="/content"/>
                            <support
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Select Support Link"
                                name="./supportLink"
                                rootPath="/content"/>
                            <developers
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Select developers Link"
                                name="./devLink"
                                rootPath="/content"/>
                            <partners
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Select Partners Link"
                                name="./partnersLink"
                                rootPath="/content"/>
                            <company-title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Select Company link"
                                name="./companyTitle"
                                rootPath="/content"/>
                        </items>
                    </column>
                </items>
            </navigation>
            <footer-links
                jcr:primaryType="nt:unstructured"
                jcr:title="Footer Links"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <copyright
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Image"
                                sling:resourceType="granite/ui/components/foundation/section">
                                <layout
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                    margin="{Boolean}false"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <copyrighttext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldLabel="Copyright Text"
                                                name="./copyright"/>
                                        </items>
                                    </column>
                                </items>
                            </copyright>
                            <lowernav
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Image"
                                sling:resourceType="granite/ui/components/foundation/section">
                                <layout
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                    margin="{Boolean}false"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <home
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldLabel="Select Home link"
                                                name="./homeLink"
                                                rootPath="/content"/>
                                            <aboutus
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldLabel="Select Aboutus Link"
                                                name="./aboutusLink"
                                                rootPath="/content"/>
                                            <faq
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldLabel="Select Faq Link"
                                                name="./faqLink"
                                                rootPath="/content"/>
                                            <contactus
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldLabel="Select Contactus Link"
                                                name="./contactusLink"
                                                rootPath="/content"/>
                                        </items>
                                    </column>
                                </items>
                            </lowernav>
                        </items>
                    </column>
                </items>
            </footer-links>
        </items>
    </content>
</jcr:root>
